package Modelos.NPC;

/**
 *
 * @author victor
 */
public class NPC {
    private int id;
    private String nombre;
    private String dialogo;
    
    public NPC(int id,String nombre, String dialogo){
    this.id=id;
    this.dialogo=dialogo;
    this.nombre=nombre;
    }

    public int getId() {
        return id;
    }

    public String getNombre() {
        return nombre;
    }

    public String getDialogo() {
        return dialogo;
    }

    public void setId(int id) {
        this.id = id;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public void setDialogo(String dialogo) {
        this.dialogo = dialogo;
    }
    
    
}
