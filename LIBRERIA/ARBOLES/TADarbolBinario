package Libreria;

/**
 *
 * @author Enock
 */
public interface TADArbolBinario<T> {
       void vaciar();
       int altura();
       void insertar(T id);
       int obtenerRaiz();
       int obtenerIzquierdo(Nodo<T> nodo);
       int obtenerDerecho(Nodo<T> nodo);
       void eliminar(T id);
       void preOrden();
       void inOrden();
       void postOrden();
       int nivelOrden(T id);
       boolean buscar(T id);
      
}
